input("Введіть два числа:")
a = float(input(''))
b = float(input(''))
s = input("Знак (+,-,*,/): ")
if s == '+':
    print(a+b)
if s == '-':
    print(a-b)
if s == '*':
    print(a*b)
if s == '/':
    print(a/b)

